{
  "name": "pixel-vibe",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "phaser-app",
  "workspaceFolder": "/pixel-vibe",
  "forwardPorts": [5173],
  "postCreateCommand": "pnpm install",
  "customizations": {
    "vscode": {
      "extensions": [
        "antfu.browse-lite",
        "continue.continue",
        "usernamehw.errorlens",
        "steoates.autoimport",
        "dbaeumer.vscode-eslint",
        "github.vscode-github-actions",
        "firsttris.vscode-jest-runner",
        "christian-kohler.path-intellisense",
        "esbenp.prettier-vscode",
        "antfu.vite",
        "antfu.vite",
        "redhat.vscode-yaml",
        "github.copilot"
      ]
    }
  },
  "remoteUser": "node"
}